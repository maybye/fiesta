#include <iostream>
#include <math.h>
#include <algorithm>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Rus");

	//задание 1
	cout << "Даны целые положительные числа A и B (A < B). Вывести все целые числа от A до B включительно." << endl;
	cout << "Введите А и В: ";
	int A, B, i, j;
	cin >> A >> B;
	for (i = A; i <= B; i++)
		for (j = 1; j <= i; j++)
			cout << i << endl;

	//задание 2
	cout << " " << endl;
	cout << "Даны положительные числа A и B (A > B). На отрезке длины A размещено максимально возможное количество отрезков длины B (без наложений)." << endl;
	cout << "Не используя операции умножения и деления, найти длину незанятой части отрезка A." << endl;
	cout << "Введите А и В: ";
	int a, b;
	cin >> a >> b;
	while (a >= b)
		a = a - b;
	cout << "Длина незанятой части отрезка А равна: " << a << endl;

	//задание 3
	cout << " " << endl;
	cout << "Дано целое число N (> 1). Вывести наименьшее из целых чисел K, для которых сумма 1 + 2 + . . . + K будет больше или равна N, и саму эту сумму." << endl;
	cout << "Введите N: ";
	int K = 0;
	int N;
	cin >> N;
	int sum = 0;
	while (sum < N)
	{
		K++;
		sum += K;			
	}
	cout << sum << K;
	
	

	//задание 4
	cout << " " << endl;
	double p, s = 1000;
	int k = 0;
	cout << "Введите процент, под который был положен вклад: " << endl;
	cin >> p;
	while (s <= 1100)
	{
		k++;
		s = s + s * p/100;	
	}
	cout << s << " " << k;

	//задание 5
	cout << " " << endl;
	cout << "Даны целые положительные числа A и B. Найти их наибольший общий делитель (НОД)." << endl;
	cout << "Введите А и В: " << endl;
	int A1, B1, C;
	C = 0;
	cin >> A1 >> B1;
	
		



}
